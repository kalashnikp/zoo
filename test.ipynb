    "import random\n",
    "\n",
    "zoo = [1,2,3,4,5]\n",
    "new_zoo = []\n",
    "\n",
    "for items in (zoo):\n",
    "    a = random.choices([1, 0], weights=[0.25, 0.75])\n",
    "    b = int(''.join(map(str,a)))\n",
    "    c = bool(b)\n",
    "    \n",
    "    if c == True:\n",
    "        new_zoo.append(items)\n",
    "\n",
    "zoo += new_zoo\n",
    "\n",
    "print(zoo)\n",
    "print(new_zoo)\n",
    "print(c, type(c))\n"
   ]
